{
  "name": "escolalms/model-fields",
  "description": "Escola LMS model-fields extension.",
  "type": "package",
  "require": {
    "php": ">=8.0",
    "laravel/framework": ">=8.0",
    "escolalms/core": "^1"
  },
  "require-dev": {
    "escolalms/auth": "^0",
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^6",
    "infection/infection": "^0",
    "nunomaduro/larastan": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Mateusz Wojczal",
      "email": "mateusz@wojczal.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\ModelFields\\": "src",
      "EscolaLms\\ModelFields\\Tests\\": "tests",
      "EscolaLms\\ModelFields\\Database\\Seeders\\": "database/seeders"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\ModelFields\\ModelFieldsServiceProvider"
      ]
    }
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
